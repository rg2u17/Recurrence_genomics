# Written for terminal interface

dx run prsice2 \
--base-assoc regenie_output_10yr_colic_to_colic.txt             # can amend to alternative GWAS summary stats file if necessary
--plink_bed ukb_cal_chr1_22_v2_merged_amended_5.bed             # needs to be run chromosome wide hence merged file
--plink_bim ukb_cal_chr1_22_v2_merged_amended_5.bim
--plink_fam ukb_cal_chr1_22_v2_merged_amended_5.fam
--trait binary
--pheno_txt pheno_file_10yr_colic_to_colic2_all.txt             # can amend to alternative phenotype file if necessary
--out colic_to_colic_10yr_total_dataset_prs                     # can amend output as necessary
